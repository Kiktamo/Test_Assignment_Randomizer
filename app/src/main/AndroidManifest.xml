<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testassignmentrandomizer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestAssignmentRandomizer">
        <activity
            android:name=".MainActivityBasic"
            android:label="@string/app_name"
            android:theme="@style/Theme.TestAssignmentRandomizer.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GradingActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".DownloadActivity" />
        <activity android:name=".TemplatesActivity" />
        <activity android:name=".TestsActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>